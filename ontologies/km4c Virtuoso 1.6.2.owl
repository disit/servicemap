     -->

	
	 <owl:Ontology rdf:about="http://www.disit.org/km4city/schema">
		<dct:title xml:lang="en">km4city, the DISIT Knowledge Model for City and Mobility</dct:title>
		<dct:description xml:lang="en">A Knowledge Model to describe a smart city, that interconnect data from infomobility service, Open Data and other source</dct:description>
		<dct:subject xml:lang="en">Ontology, Semantic SEO, RDFa, Linked Data, RDF, Semantic Web, Smart Recommender Systems, Smart City, Transportation Network, Smart Mobility, Citizens Services, Semantic Model, knowledge model</dct:subject>
		<vann:preferredNamespacePrefix>km4c</vann:preferredNamespacePrefix>
		<vann:preferredNamespaceUri>http://www.disit.org/km4city/schema</vann:preferredNamespaceUri>
		<rdfs:label xml:lang="en">km4city, the DISIT Knowledge Model for City and Mobility</rdfs:label>
		<dct:creator xml:lang="en">Paolo Nesi</dct:creator>
		<dct:creator xml:lang="en">Pierfrancesco Bellini</dct:creator>
		<dct:creator xml:lang="en">Nadia Rauch</dct:creator>
		<dct:publisher>DISIT Lab, University of Florence, Italy</dct:publisher>
		<owl:versionInfo xml:lang="en">V 1.6.2 RDFSPlus Adaptation, Release 2015-09-09</owl:versionInfo>
		<dct:license rdf:resource="https://creativecommons.org/licenses/by-sa/3.0/it/"/>
		<dct:rights xml:lang="en">The Knowledge Model for the City - km4city - is available under Creative Commons Attribution-ShareAlike 3.0 Unported license</dct:rights>
		<rdfs:comment xml:lang="en">The Knowledge Model aims at enabling interconnection, storage and the next interrogation of data from many different sources, such as various portals of the Tuscan region (MIIC, Muoversi in Toscana, Osservatorio dei Trasporti), Open Data provoded by individual municipalities (mainly Florence).</rdfs:comment>
		<dct:contributor xml:lang="en">Municipality of Florence, Tuscany Region</dct:contributor>
		<foaf:homepage rdf:resource="http://www.disit.org/"/>
	</owl:Ontology>
	 
    <!-- http://www.disit.org/km4city/schema#containsElement -->
	
    <owl:ObjectProperty rdf:about="&km4c;containsElement">
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
        <rdfs:domain rdf:resource="&km4c;Road"/>
		<owl:inverseOf rdf:resource="&km4c;isPartOfRoad"/>
		<rdfs:label xml:lang="en">Contains element</rdfs:label>
		<rdfs:label xml:lang="it">Contiene elemento</rdfs:label>
		<rdfs:comment>A road is formed by more elements road</rdfs:comment>
    </owl:ObjectProperty>
	
	
    <!-- http://www.disit.org/km4city/schema#coincideWith -->

    <owl:ObjectProperty rdf:about="&km4c;coincideWith">
        <rdfs:range rdf:resource="&km4c;AdministrativeRoad"/>
        <rdfs:domain rdf:resource="&km4c;Road"/>
		<rdfs:label xml:lang="en">Coincide with</rdfs:label>
		<rdfs:label xml:lang="it">Coincide con</rdfs:label>
		<rdfs:comment>there is no exact match between Road and AdministrativeRoad</rdfs:comment>
    </owl:ObjectProperty>
	
    <!-- http://www.disit.org/km4city/schema#correspondsTo -->

    <owl:ObjectProperty rdf:about="&km4c;correspondsTo">
        <rdfs:range rdf:resource="&km4c;Road"/>
        <rdfs:domain rdf:resource="&km4c;AdministrativeRoad"/>
		<rdfs:label xml:lang="en">Corresponds to</rdfs:label>
		<rdfs:label xml:lang="it">Corrisponde a</rdfs:label>
		<rdfs:comment>there is no exact match between Road and AdministrativeRoad</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#managingAuthority -->

    <owl:ObjectProperty rdf:about="&km4c;managingAuthority">
        <rdfs:range rdf:resource="&km4c;Pa"/>
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Managing authority</rdfs:label>
		<rdfs:label xml:lang="it">Autorita' gestionale</rdfs:label>
		<rdfs:comment>the authority that deals with the road elements management</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#ownerAuthority -->

    <owl:ObjectProperty rdf:about="&km4c;ownerAuthority">
        <rdfs:range rdf:resource="&km4c;Pa"/>
        <rdfs:domain rdf:resource="&km4c;AdministrativeRoad"/>
		<rdfs:label xml:lang="en">Owner authority</rdfs:label>
		<rdfs:label xml:lang="it">Autorita' proprietaria</rdfs:label>
		<rdfs:comment>the authority that owns the administrative road</rdfs:comment>
    </owl:ObjectProperty>
	
	
	<!-- http://www.disit.org/km4city/schema#inMunicipalityOf -->

    <owl:ObjectProperty rdf:about="&km4c;inMunicipalityOf">
        <rdfs:range rdf:resource="&km4c;Pa"/>
        <rdfs:domain rdf:resource="&km4c;Road"/>
		<rdfs:label xml:lang="en">In municipality of</rdfs:label>
		<rdfs:label xml:lang="it">Nel comune</rdfs:label>
		<rdfs:comment>municipality to which the road belongs</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#isPartOfRoad -->

    <owl:ObjectProperty rdf:about="&km4c;isPartOfRoad">
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
		<rdfs:range rdf:resource="&km4c;Road"/>
        <owl:inverseOf rdf:resource="&km4c;containsElement"/>
		<rdfs:label xml:lang="en">Is part of Road</rdfs:label>
		<rdfs:label xml:lang="it">E' parte della strada</rdfs:label>
		<rdfs:comment>More road elements compose a road</rdfs:comment>
    </owl:ObjectProperty>
	
	
	<!-- http://www.disit.org/km4city/schema#formingAdminRoad -->

    <owl:ObjectProperty rdf:about="&km4c;formingAdminRoad">
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
		<rdfs:range rdf:resource="&km4c;AdministrativeRoad"/>
        <owl:inverseOf rdf:resource="&km4c;hasRoadElement"/>
		<rdfs:label xml:lang="en">Forming administrative road</rdfs:label>
		<rdfs:label xml:lang="it">Forma una strada amministrativa</rdfs:label>
		<rdfs:comment>more road elements form an administrative road</rdfs:comment>
    </owl:ObjectProperty>
	
	<!-- http://www.disit.org/km4city/schema#hasRoadElement -->

    <owl:ObjectProperty rdf:about="&km4c;hasRoadElement">
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
        <rdfs:domain rdf:resource="&km4c;AdministrativeRoad"/>
		<owl:inverseOf rdf:resource="&km4c;formingAdminRoad"/>
		<rdfs:label xml:lang="en">Has road element</rdfs:label>
		<rdfs:label xml:lang="it">Ha il road element</rdfs:label>
		<rdfs:comment>more road elements form an administrative road</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#isPartOfProvince -->

    <owl:ObjectProperty rdf:about="&km4c;isPartOfProvince">
        <rdfs:domain rdf:resource="&km4c;Municipality"/>
        <rdfs:range rdf:resource="&km4c;Province"/>
		<owl:inverseOf rdf:resource="&km4c;hasMunicipality"/>
		<rdfs:label xml:lang="en">Is part of province</rdfs:label>
		<rdfs:label xml:lang="it">E' parte della provincia</rdfs:label>
		<rdfs:comment>a province is formed by more municipalities</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#isPartOfRegion -->

    <owl:ObjectProperty rdf:about="&km4c;isPartOfRegion">
        <rdfs:domain rdf:resource="&km4c;Province"/>
        <rdfs:range rdf:resource="&km4c;Region"/>
		<owl:inverseOf rdf:resource="&km4c;hasProvince"/>
		<rdfs:label xml:lang="en">Is part of region</rdfs:label>
		<rdfs:label xml:lang="it">E' parte della regione</rdfs:label>
		<rdfs:comment>in a region there are more than one province</rdfs:comment>
    </owl:ObjectProperty>
     
	
    <!-- http://www.disit.org/km4city/schema#hasMunicipality -->

    <owl:ObjectProperty rdf:about="&km4c;hasMunicipality">
        <rdfs:range rdf:resource="&km4c;Municipality"/>
        <rdfs:domain rdf:resource="&km4c;Province"/>
        <owl:inverseOf rdf:resource="&km4c;isPartOfProvince"/>
		<rdfs:label xml:lang="en">Has municipality</rdfs:label>
		<rdfs:label xml:lang="it">Ha il comune</rdfs:label>
		<rdfs:comment>a province is formed by more municipalities</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://www.disit.org/km4city/schema#hasProvince -->

    <owl:ObjectProperty rdf:about="&km4c;hasProvince">
        <rdfs:range rdf:resource="&km4c;Province"/>
        <rdfs:domain rdf:resource="&km4c;Region"/>
        <owl:inverseOf rdf:resource="&km4c;isPartOfRegion"/>
		<rdfs:label xml:lang="en">Has province</rdfs:label>
		<rdfs:label xml:lang="it">Ha la provincia</rdfs:label>
		<rdfs:comment>in a region there are more than one province</rdfs:comment>
    </owl:ObjectProperty>
	

    <!-- http://www.disit.org/km4city/schema#hasDistrict -->

    <owl:ObjectProperty rdf:about="&km4c;hasDistrict">
        <rdfs:range rdf:resource="&km4c;District"/>
        <rdfs:domain rdf:resource="&km4c;Municipality"/>
        <owl:inverseOf rdf:resource="&km4c;belongToMunicipality"/>
		<rdfs:label xml:lang="en">Has district</rdfs:label>
		<rdfs:label xml:lang="it">Ha il quartiere</rdfs:label>
		<rdfs:comment>A municipality can be divided in districts</rdfs:comment>
    </owl:ObjectProperty>	
  

    <!-- http://www.disit.org/km4city/schema#belongToMunicipality -->

    <owl:ObjectProperty rdf:about="&km4c;belongToMunicipality">
        <rdfs:range rdf:resource="&km4c;Municipality"/>
        <rdfs:domain rdf:resource="&km4c;District"/>
        <owl:inverseOf rdf:resource="&km4c;hasDistrict"/>
		<rdfs:label xml:lang="en">Belong to municipality</rdfs:label>
		<rdfs:label xml:lang="it">Appartiene al comune</rdfs:label>
		<rdfs:comment>A municipality can be divided in districts</rdfs:comment>
    </owl:ObjectProperty>
	
	
    <!-- http://www.disit.org/km4city/schema#endsAtNode -->

    <owl:ObjectProperty rdf:about="&km4c;endsAtNode">
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
        <rdfs:range rdf:resource="&km4c;Node"/>
		<rdfs:label xml:lang="en">Ends at node</rdfs:label>
		<rdfs:label xml:lang="it">Finisce al nodo</rdfs:label>
		<rdfs:comment>a road element is delimited by 2 nodes</rdfs:comment>
    </owl:ObjectProperty>
	

    <!-- http://www.disit.org/km4city/schema#startsAtNode -->

    <owl:ObjectProperty rdf:about="&km4c;startsAtNode">
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
        <rdfs:range rdf:resource="&km4c;Node"/>
		<rdfs:label xml:lang="en">Starts at node</rdfs:label>
		<rdfs:label xml:lang="it">Inizia al nodo</rdfs:label>
		<rdfs:comment>a road element is delimited by 2 nodes</rdfs:comment>
    </owl:ObjectProperty>


    <!-- http://www.disit.org/km4city/schema#hasStreetNumber -->

    <owl:ObjectProperty rdf:about="&km4c;hasStreetNumber">
        <rdfs:domain rdf:resource="&km4c;Road"/>
        <rdfs:range rdf:resource="&km4c;StreetNumber"/>
		<owl:inverseOf rdf:resource="&km4c;belongToRoad"/>
		<rdfs:label xml:lang="en">Has street number</rdfs:label>
		<rdfs:label xml:lang="it">Ha numero civico</rdfs:label>
		<rdfs:comment>each road has street numbers</rdfs:comment>
    </owl:ObjectProperty>	
	
	
    <!-- http://www.disit.org/km4city/schema#belongToRoad -->

    <owl:ObjectProperty rdf:about="&km4c;belongToRoad">
        <rdfs:domain rdf:resource="&km4c;StreetNumber"/>
        <rdfs:range rdf:resource="&km4c;Road"/>
		<owl:inverseOf rdf:resource="&km4c;hasStreetNumber"/>
		<rdfs:label xml:lang="en">Belong to road</rdfs:label>
		<rdfs:label xml:lang="it">Appartiene alla strada</rdfs:label>
		<rdfs:comment>each road has street numbers</rdfs:comment>
    </owl:ObjectProperty>		
	
	
	<!-- http://www.disit.org/km4city/schema#placedInElement -->

    <owl:ObjectProperty rdf:about="&km4c;placedInElement">
        <rdfs:domain rdf:resource="&km4c;Entry"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Placed in element</rdfs:label>
		<rdfs:label xml:lang="it">Posizionato nell'elemento</rdfs:label>
		<rdfs:comment>each entry need to refers to a road</rdfs:comment>
    </owl:ObjectProperty>		
	

	<!-- http://www.disit.org/km4city/schema#isInElement -->

    <owl:ObjectProperty rdf:about="&km4c;isInElement">
        <rdfs:domain rdf:resource="&km4c;Milestone"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Is in element</rdfs:label>
		<rdfs:label xml:lang="it">E' nell'elemento</rdfs:label>
		<rdfs:comment>each milestone need to refers to a road</rdfs:comment>
    </owl:ObjectProperty>		


    <!-- http://www.disit.org/km4city/schema#hasRule -->

    <owl:ObjectProperty rdf:about="&km4c;hasRule">
        <rdfs:domain rdf:resource="&km4c;RoadElement"/>
        <rdfs:range rdf:resource="&km4c;EntryRule"/>
		<owl:inverseOf rdf:resource="&km4c;accessToElement"/>
		<rdfs:label xml:lang="en">Has rule</rdfs:label>
		<rdfs:label xml:lang="it">Ha regola</rdfs:label>
		<rdfs:comment>some road elements subject to rules of access</rdfs:comment>
    </owl:ObjectProperty>	


    <!-- http://www.disit.org/km4city/schema#accessToElement -->

    <owl:ObjectProperty rdf:about="&km4c;accessToElement">
        <rdfs:domain rdf:resource="&km4c;EntryRule"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<owl:inverseOf rdf:resource="&km4c;hasRule"/>
		<rdfs:label xml:lang="en">Access to element</rdfs:label>
		<rdfs:label xml:lang="it">Accede all'elemento</rdfs:label>
		<rdfs:comment>an entry rule refers to a specific Road element</rdfs:comment>
    </owl:ObjectProperty>		


	<!-- http://www.disit.org/km4city/schema#hasManeuver -->

    <owl:ObjectProperty rdf:about="&km4c;hasManeuver">
        <rdfs:domain rdf:resource="&km4c;EntryRule"/>
        <rdfs:range rdf:resource="&km4c;Maneuver"/>
		<rdfs:label xml:lang="en">Has maneuver</rdfs:label>
		<rdfs:label xml:lang="it">Ha la manovra</rdfs:label>
		<rdfs:comment>an entry rule can be connected to a specific maneuver</rdfs:comment>
    </owl:ObjectProperty>		
	

	<!-- http://www.disit.org/km4city/schema#concerningNode -->

    <owl:ObjectProperty rdf:about="&km4c;concerningNode">
        <rdfs:domain rdf:resource="&km4c;Maneuver"/>
        <rdfs:range rdf:resource="&km4c;Node"/>
		<rdfs:label xml:lang="en">Concerning node</rdfs:label>
		<rdfs:label xml:lang="it">Riguarda il nodo</rdfs:label>
		<rdfs:comment>a maneuver takes place in correspondence of a node</rdfs:comment>
    </owl:ObjectProperty>	

	<!-- http://www.disit.org/km4city/schema#hasFirstElem -->

    <owl:ObjectProperty rdf:about="&km4c;hasFirstElem">
        <rdfs:domain rdf:resource="&km4c;Maneuver"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Has first element</rdfs:label>
		<rdfs:label xml:lang="it">Ha il primo elemento</rdfs:label>
		<rdfs:comment>the first element concerned the maneuver</rdfs:comment>
    </owl:ObjectProperty>	

	<!-- http://www.disit.org/km4city/schema#hasSecondElem -->

    <owl:ObjectProperty rdf:about="&km4c;hasSecondElem">
        <rdfs:domain rdf:resource="&km4c;Maneuver"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Has second element</rdfs:label>
		<rdfs:label xml:lang="it">Ha il secondo elemento</rdfs:label>
		<rdfs:comment>the second element concerned the maneuver</rdfs:comment>
    </owl:ObjectProperty>	


	<!-- http://www.disit.org/km4city/schema#hasThirdElem -->

    <owl:ObjectProperty rdf:about="&km4c;hasThirdElem">
        <rdfs:domain rdf:resource="&km4c;Maneuver"/>
        <rdfs:range rdf:resource="&km4c;RoadElement"/>
		<rdfs:label xml:lang="en">Has third element</rdfs:label>
		<rdfs:label xml:lang="it">Ha il terzo elemento</rdfs:label>
		<rdfs:comment>the third element concerned the maneuver</rdfs:comment>
    </owl:ObjectProperty>		
	
	
	<!-- http://www.disit.org/km4city/schema#hasInternalAccess -->

    <owl:ObjectProperty rdf:about="&km4c;hasInternalAccess">
        <rdfs:domain rdf:resource="&km4c;StreetNumber"/>
        <rdfs:range rdf:resource="&km4c;Entry"/>
		<rdfs:label xml:lang="en">Has internal access</rdfs:label>
		<rdfs:label xml:lang="it">Ha accesso interno</rdfs:label>
		<rdfs:comment>it corresponds to the door of the building</rdfs:comment>
    </owl:ObjectProperty>		
	
	
	<!-- http://www.disit.org/km4city/schema#hasExternalAccess -->

    <owl:ObjectProperty rdf:about="&km4c;hasExternalAccess">
        <rdfs:domain rdf:resource="&km4c;StreetNumber"/>
        <rdfs:range rdf:resource="&km4c;Entry"/>
		<rdfs:label xml:lang="en">Has external access</rdfs:label>
		<rdfs:label xml:lang="it">Ha accesso esterno</rdfs:label>
		<rdfs:comment>it corresponds to the outer gate of the building</rdfs:comment>
    </owl:ObjectProperty>		
	
	
	<!-- http://www.disit.org/km4city/schema#hasAccess -->

    <owl:ObjectProperty rdf:about="&km4c;hasAccess">
		<rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&km4c;Service"/>
                    <rdf:Description rdf:about="&gr;BusinessEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&km4c;Entry"/>
		<rdfs:label xml:lang="en">Has acceess</rdfs:label>
		<rdfs:label xml:lang="it">Ha accesso</rdfs:label>
		<rdfs:comment>point corresponding to the house number of service</rdfs:comment>
    </owl:ObjectProperty>	


	<!-- http://www.disit.org/km4city/schema#refersToMunicipality -->

    <owl:ObjectProperty rdf:about="&km4c;refersToMunicipality">
        <rdfs:domain rdf:resource="&km4c;WeatherReport"/>
        <rdfs:range rdf:resource="&km4c;Municipality"/>
		<owl:inverseOf rdf:resource="&km4c;hasWeatherReport"/>
		<rdfs:label xml:lang="en">Refers to municipality</rdfs:label>
		<rdfs:label xml:lang="it">Si riferisce al comune</rdfs:label>
		<rdfs:comment>specifies the municipality for the weather forecast</rdfs:comment>
    </owl:ObjectProperty>	
	
	
	<!-- http://www.disit.org/km4city/schema#hasWeatherReport -->

    <owl:ObjectProperty rdf:about="&km4c;hasWeatherReport">
        <rdfs:domain rdf:resource="&km4c;Municipality"/>
        <rdfs:range rdf:resource="&km4c;WeatherReport"/>
		<owl:inverseOf rdf:resource="&km4c;refersToMunicipality"/>
		<rdfs:label xml:lang="en">Has weather report</rdfs:label>
		<rdfs:label xml:lang="it">Ha report meteorologico</rdfs:label>
		<rdfs:comment>each weather forecasts is connected to a municipality</rdfs:comment>
    </owl:ObjectProperty>	


	<!-- http://www.disit.org/km4city/schema#hasPrediction -->

    <owl:ObjectProperty rdf:about="&km4c;hasPrediction">
        <rdfs:domain rdf:resource="&km4c;WeatherReport"/>
        <rdfs:range rdf:resource="&km4c;WeatherPrediction"/>
		<rdfs:label xml:lang="en">Has prediction</rdfs:label>
		<rdfs:label xml:lang="it">Ha previsione</rdfs:label>
		<rdfs:comment>a report is made up of more predictions</rdfs:comment>
    </owl:ObjectProperty>	
	
	
	<!-- http://www.disit.org/km4city/schema#observeCarPark -->
	
    <owl:ObjectProperty rdf:about="&km4c;observeCarPark">
        <rdfs:domain rdf:resource="&km4c;CarParkSensor"/>
